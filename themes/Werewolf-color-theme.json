	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#778BA6"
			}
		},
		{
			"name": "Variables, Misc",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.brace",
				"meta.object-literal.key",
				"meta",
				"source.shell",
				"text.html"
			],
			"settings": {
				"foreground": "#BCCEDE"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#BCCEDE"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F582A9"
			}
		},
		{
			"name": "Keyword, Storage",
